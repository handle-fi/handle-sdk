{
    "plugins": [
      "jsdoc-plugin-typescript"
    ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.ts(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "typescript": {
      "moduleRoot": "src"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}