{
    "plugins": [
      "jsdoc-plugin-typescript"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["."],
        "includePattern": ".(jsx|js|ts|tsx)$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "typescript": {
      "moduleRoot": "src"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}